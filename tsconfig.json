{
   "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "lib": ["es2020", "dom"],
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "outDir": "dist",
      // "strict": true,
      "incremental": true,
      "typeRoots": ["./node_modules/@types", "./src/types"]
   },
   "include": ["src/**/*"],
   "exclude": ["node_modules", "dist"]
}
